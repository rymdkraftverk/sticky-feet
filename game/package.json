{
  "name": "game",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "parcel -p 8081 --hmr-port 8082 src/index.html",
    "build": "rm -rf dist && parcel build src/index.html --public-url ./",
    "typecheck": "tsc --module commonjs --allowJs --checkJs --noEmit --target es2016 src/*.js",
    "lint": "eslint .",
    "munch": "muncher --input src/asset --output static/spritesheet/fight"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "muncher": "0.0.4",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "l1": "^0.6.0-1",
    "matter-js": "^0.14.2",
    "pixi.js": "^5.1.1",
    "poly-decomp": "^0.3.0",
    "qrcode": "^1.4.1",
    "ramda": "^0.26.1",
    "rkv-signaling": "0.0.3"
  }
}
